<div class="message <%= messageType %>" title="<%= timestamp %>">
	<span class="tripcode"><%= tripcode %></span>
	<span class="handle"><%= handle %></span>
	<div>
		<% if (typeof file != "undefined") { %>
		<div class="filedownload">
			<% if (typeof inlineView != "undefined") { %>
				<% if (file.type == "image") { %>
				<span><%= file.name %></span>
				<img title="<%= file.name %>" alt="<%= file.name %>" src="data:<%= file.mimetype %>;base64,<%= file.data %>" />
				<% } else if (file.type == "video") { %>
				<span><%= file.name %></span>
				<video controls title="<%= file.name %>" src="data:<%= file.mimetype %>;base64,<%= file.data %>"></video>
				<% } else if (file.type == "audio") { %>
				<span><%= file.name %></span>
				<audio controls title="<%= file.name %>">
					<source src="data:<%= file.mimetype %>;base64,<%= file.data %>">
				</audio>
				<% } else { %>
				<a download="<%= file.name %>" href="data:<%= file.mimetype %>;base64,<%= file.data %>"><span><%= file.name %></span></a>
				<% } %>
			<% } else { %>
			<a download="<%= file.name %>" href="data:<%= file.mimetype %>;base64,<%= file.data %>"><span><%= file.name %></span></a>
			<% } %>
		</div>
		<% } %>
		<% if (typeof message != "undefined" && message != "") { %>
			<span class="message-text"><%= message %></span>
		<% } %>
	</div>
</div>